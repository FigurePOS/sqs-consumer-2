{
    "name": "@figurepos/sqs-consumer",
    "version": "5.9.0",
    "description": "SQS Consumer",
    "scripts": {
        "build": "yarn run clean && tsc",
        "clean": "rm -fr lib/*",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "posttest": "yarn run lint",
        "prepublish": "yarn run build",
        "pretest": "yarn run build",
        "prettier:check": "./node_modules/.bin/prettier --check '**/*.js' '**/*.json' '**/*.ts'",
        "prettier:fix": "./node_modules/.bin/prettier --write '**/*.js' '**/*.json' '**/*.ts'",
        "test": "mocha --recursive --full-trace --exit",
        "release": "yarn build && yarn release-it",
        "watch": "tsc --watch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/FigurePOS/sqs-consumer-2.git"
    },
    "homepage": "https://github.com/FigurePOS/sqs-consumer-2",
    "keywords": [
        "sqs",
        "queue",
        "consumer"
    ],
    "license": "Apache-2.0",
    "devDependencies": {
        "@aws-sdk/client-sqs": "3.391.0",
        "@figuredev/eslint-config-node": "^2.0.1",
        "@figuredev/prettier-config": "^2.1.0",
        "@release-it/conventional-changelog": "^9.0.3",
        "@types/chai": "^4.2.11",
        "@types/debug": "^4.1.5",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.0.13",
        "@types/sinon": "^9.0.4",
        "chai": "^4.2.0",
        "eslint": "^9.16.0",
        "mocha": "^8.0.1",
        "p-event": "^4.2.0",
        "prettier": "^3.4.2",
        "release-it": "^17.10.0",
        "sinon": "^9.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "debug": "^4.1.1"
    },
    "peerDependencies": {
        "@aws-sdk/client-sqs": "^3.391.0"
    },
    "mocha": {
        "spec": "tests/**/**/*.ts",
        "require": "ts-node/register"
    },
    "prettier": "@figuredev/prettier-config",
    "publishConfig": {
        "access": "restricted",
        "registry": "https://npm.pkg.github.com"
    },
    "packageManager": "yarn@4.5.3"
}
